{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.3.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js",
        "jquery.validate.unobtrusive.js"
      ]
    },
    {
      "library": "cldrjs@0.5.0",
      "destination": "wwwroot/lib/cldrjs/",
      "files": [
        "cldr.min.js",
        "cldr/supplemental.min.js",
        "cldr/event.min.js"
      ]
    },
    {
      "library": "globalize@1.4.0",
      "destination": "wwwroot/lib/globalize/",
      "files": [
        "globalize.min.js",
        "globalize/number.min.js",
        "globalize/plural.min.js",
        "globalize/currency.min.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_et.min.js"
      ]
    }
  ]
}